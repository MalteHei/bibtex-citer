{
	"name": "bibtex-citer",
	"displayName": "BibTeX Citer",
	"description": "Intellisense for Keys from your BibTeX Libraries.",
	"version": "1.1.1",
	"publisher": "maltehei",
	"repository": "https://github.com/MalteHei/bibtex-citer",
	"bugs": "https://github.com/MalteHei/bibtex-citer/issues",
	"engines": {
		"vscode": "^1.66.0"
	},
	"categories": [
		"Language Packs",
		"Education"
	],
	"keywords": [
		"latex",
		"biblatex",
		"cite",
		"intellisense",
		"code completion"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "bibtex-citer.readLibraries",
				"title": "BibTeX-Citer: Read Keys from .bib-Files"
			}
		],
		"languages": [
			{
				"id": "latex",
				"aliases": [
					"LaTeX",
					"TeX"
				],
				"extensions": [
					".tex",
					".bib"
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.66.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.21.0",
		"@typescript-eslint/parser": "^5.21.0",
		"eslint": "^8.14.0",
		"glob": "^8.0.1",
		"mocha": "^9.2.2",
		"typescript": "^4.6.4",
		"@vscode/test-electron": "^2.1.3"
	}
}
